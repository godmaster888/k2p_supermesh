	help
	  Can build version '5.5.1', '5.9.0'.
	  default: '5.5.1'.

config BR2_PACKAGE_STRONGSWAN_USE_STORAGE
	bool
	default y if BR2_PACKAGE_STRONGSWAN_VERSION_5_9_0
	help
	  Use storage to get strongswan package.

config BR2_PACKAGE_STRONGSWAN_WITH_HOST_GPERF
	bool
	default y if BR2_PACKAGE_STRONGSWAN_VERSION_5_9_0
	help
